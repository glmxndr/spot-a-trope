<h1>Listing tocentries</h1>

<table>
  <tr>
    <th>Opus</th>
    <th>Name</th>
    <th>Code</th>
    <th>Type</th>
  </tr>

<% @tocentries.each do |tocentry| %>
  <tr>
    <td><%=h tocentry.opus_id %></td>
    <td><%=h tocentry.name %></td>
    <td><%=h tocentry.code %></td>
    <td><%=h tocentry.type %></td>
    <td><%= link_to 'Show', tocentry %></td>
    <td><%= link_to 'Edit', edit_tocentry_path(tocentry) %></td>
    <td><%= link_to 'Destroy', tocentry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tocentry', new_tocentry_path %>
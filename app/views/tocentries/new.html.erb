<h1>New tocentry</h1>

<% form_for(@tocentry) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :opus_id %><br />
    <%= f.text_field :opus_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', tocentries_path %>